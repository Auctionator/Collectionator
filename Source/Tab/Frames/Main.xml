<Ui xmlns="http://www.blizzard.com/wow/ui/">
  <Frame
    name="PMTabFrameTemplate"
    virtual="true"
    inherits="AuctionatorTabFrameTemplate"
    parentKey="PMTabFrame"
    mixin="PMTabMixin"
  >
    <Frames>
      <Button inherits="UIPanelDynamicResizeButtonTemplate" text="Process FS" parentKey="ProcessFS">
        <Anchors>
          <Anchor point="TOPLEFT" relativeKey="$parent" relativePoint="TOPLEFT" y="-10" x="10" />
        </Anchors>
        <Scripts>
          <OnClick>
            self:GetParent():ProcessFSClicked()
          </OnClick>
        </Scripts>
      </Button>
    </Frames>
  </Frame>
</Ui>
