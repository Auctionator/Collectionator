<Ui xmlns="http://www.blizzard.com/wow/ui/">
  <Frame
    name="CollectionatorRowTemplate"
    mixin="CollectionatorRowMixin"
    inherits="AuctionatorResultsRowTemplate"
    virtual="true"
    enableMouse="true"
  >
    <Layers>
      <Layer level="OVERLAY">
        <Texture parentKey="NormalTexture" atlas="auctionhouse-rowstripe-1">
          <Anchors>
            <Anchor point="TOPLEFT" relativePoint="TOPLEFT"/>
            <Anchor point="BOTTOMRIGHT" relativePoint="BOTTOMRIGHT"/>
          </Anchors>
        </Texture>
      </Layer>
    </Layers>
    <Scripts>
      <OnEvent method="OnEvent"/>
    </Scripts>
  </Frame>

  <Frame
    name="CollectionatorTMogRowTemplate"
    mixin="CollectionatorTMogRowMixin"
    inherits="CollectionatorRowTemplate"
    virtual="true"
    />

  <Frame
    name="CollectionatorPetRowTemplate"
    mixin="CollectionatorPetRowMixin"
    inherits="CollectionatorRowTemplate"
    virtual="true"
    />

  <Frame
    name="CollectionatorToyMountRowTemplate"
    mixin="CollectionatorToyMountRowMixin"
    inherits="CollectionatorRowTemplate"
    virtual="true"
    />
</Ui>
